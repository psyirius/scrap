{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "quickjs",
            "request": "launch",
            "name": "Launch QuickJS",
            "runtimeExecutable": "${workspaceFolder}/build/debug/mingw64/qjs.exe",
            "program": "${workspaceFolder}/tests/experimental/debugger.js"
        },
        {
            "type": "quickjs",
            "request": "attach",
            "name": "Attach QuickJS",
            "localRoot": "${workspaceFolder}/",
            "mode": "listen",
            "port": 5555
        }
    ]
}